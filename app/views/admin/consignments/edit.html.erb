<%= render :partial => 'admin/shared/configuration_menu' %>
 
<h1><%= t("editing_consignment") %></h1>
<%= error_messages_for :consignment %>
 
<% form_for(:consignment, :url => object_url, :html => { :method => :put }) do |f| -%>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%=submit_tag t("update")%> or <%= link_to t("cancel"), admin_consignments_path%>
  </p>
<% end %>